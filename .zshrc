# Aliases
alias c="code ."
alias o="open ."

alias cat="bat --paging=never"

alias ls="eza -a --icons --no-user --no-time --group-directories-first"
alias l="ls -1"
alias ll="ls -l"

alias g="git"
alias gl="git pull"
alias gf="git fetch"
alias gco="git checkout"
alias gcp="git cherry-pick"

export EDITOR=nano
export VISUAL="$EDITOR"